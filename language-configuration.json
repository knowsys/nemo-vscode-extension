{
    "comments": {
        "lineComment": "%"
    },

    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],

    "autoClosingPairs": [
        { "open": "{", "close": "}", "notIn": ["string", "comment"] },
        { "open": "[", "close": "]", "notIn": ["string", "comment"] },
        { "open": "(", "close": ")", "notIn": ["string", "comment"] },
        { "open": "\"", "close": "\"", "notIn": ["string", "comment"] },
        { "open": "f\"", "close": "\"", "notIn": ["string", "comment"] },
        { "open": "'", "close": "'", "notIn": ["string", "comment"] },
        { "open": "<", "close": ">", "notIn": ["string", "comment"] }
    ],

    "surroundingPairs": [
        { "open": "{", "close": "}" },
        { "open": "[", "close": "]" },
        { "open": "(", "close": ")" },
        { "open": "\"", "close": "\"" },
        { "open": "'", "close": "'" },
        { "open": "<", "close": ">" }
    ]
}
